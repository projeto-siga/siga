+<script>
+	function tamanho() {
+		var i = tamanho2();
+		if (i < 0) {
+			i = 0
+		}
+		;
+		document.getElementById("Qtd").innerText = 'Restam ' + i
+				+ ' Caracteres';
+	}
+
+	function tamanho2() {
+		nota = new String();
+		nota = this.anotar.descrMov.value;
+		var i = 255 - nota.length;
+		return i;
+	}
+	function corrige() {
+		if (tamanho2() < 0) {
+			alert('Descri√ß√£o com mais de 255 caracteres');
+			nota = new String();
+			nota = document.getElementById("descrMov").value;
