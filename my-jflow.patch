+		var e2 = document.getElementById("procedimento");
+		var procedimento = e2.options[e2.selectedIndex].value;
+		window.location.href = '${linkTo[DesignacaoController].pesquisar}'.replace(/\/\/$/, '/' + orgao + '/' + procedimento);
+	}
